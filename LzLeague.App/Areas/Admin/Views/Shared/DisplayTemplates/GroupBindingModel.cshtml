@model LzLeague.Common.AdminBindingModels.GroupBindingModel

<table class="table table-striped table-responsive-sm shadow-lg" style="margin-top: 40px">
    <thead>
    <tr><th style="border: 0"><h5>Group <b>@Model.Name</b></h5></th></tr>
    </thead>
    <thead>
    <tr>
        <th scope="col">Team</th>
        <th scope="col">Wins</th>
        <th scope="col">Loses</th>
        <th scope="col">Draws</th>
        <th scope="col">Points</th>
    </tr>
    </thead>
    <tbody>
    @Html.DisplayFor(model => model.Teams)
    </tbody>
</table>